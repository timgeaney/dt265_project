<li id="<%= feed_item.id %>">
  <%= link_to gravatar_for(feed_item.user), feed_item.user %>
    <span class="user">
       <%= link_to feed_item.user.name, feed_item.title %>
    <%=   feed_item.latitude %>
    <%=   feed_item.longitude %>


    </span>
    <span class="content"><%= feed_item.title %></span>
    <span class="timestamp">
      Added <%= time_ago_in_words(feed_item.created_at) %> ago.
    </span>
  <% if current_user?(feed_item.user) %>
    <%= link_to "delete", feed_item, method: :delete,
                                      
                                     data: { confirm: "You sure?" },
                                     title: feed_item.title %>

  <a href='' onclick="setLatLng(<%= feed_item.latitude%>, <%=   feed_item.longitude %>); return false;">
  Go To
  </a>


  <% end %>




</li>
